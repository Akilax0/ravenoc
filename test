  // AXI Interface - MOSI
  // Write Address channel
  input  logic                                   noc_out_in_awid,
  input  axi_addr_t                              noc_out_in_awaddr,
  input  logic        [`AXI_ALEN_WIDTH-1:0]      noc_out_in_awlen,
  input  asize_t                                 noc_out_in_awsize,
  input  aburst_t                                noc_out_in_awburst,
  input  logic                                   noc_out_in_awlock,
  input  logic        [3:0]                      noc_out_in_awcache,
  input  logic        [2:0]                      noc_out_in_awprot,
  input  logic        [3:0]                      noc_out_in_awqos,
  input  logic        [3:0]                      noc_out_in_awregion,
  input  logic        [`AXI_USER_REQ_WIDTH-1:0]  noc_out_in_awuser,
  input  logic                                   noc_out_in_awvalid,
  // Write Data channel
  input  logic                                   noc_out_in_wid,
  input  logic        [`AXI_DATA_WIDTH-1:0]      noc_out_in_wdata,
  input  logic        [(`AXI_DATA_WIDTH/8)-1:0]  noc_out_in_wstrb,
  input  logic                                   noc_out_in_wlast,
  input  logic        [`AXI_USER_DATA_WIDTH-1:0] noc_out_in_wuser,
  input  logic                                   noc_out_in_wvalid,
  // Write Response channel
  input  logic                                   noc_out_in_bready,
  // Read Address channel
  input  logic                                   noc_out_in_arid,
  input  axi_addr_t                              noc_out_in_araddr,
  input  logic        [`AXI_ALEN_WIDTH-1:0]      noc_out_in_arlen,
  input  asize_t                                 noc_out_in_arsize,
  input  aburst_t                                noc_out_in_arburst,
  input  logic                                   noc_out_in_arlock,
  input  logic        [3:0]                      noc_out_in_arcache,
  input  logic        [2:0]                      noc_out_in_arprot,
  input  logic        [3:0]                      noc_out_in_arqos,
  input  logic        [3:0]                      noc_out_in_arregion,
  input  logic        [`AXI_USER_REQ_WIDTH-1:0]  noc_out_in_aruser,
  input  logic                                   noc_out_in_arvalid,
  // Read Data channel
  input  logic                                   noc_out_in_rready,

  // AXI Interface - MISO
  // Write Addr channel
  output logic                                   noc_out_in_awready,
  // Write Data channel
  output logic                                   noc_out_in_wready,
  // Write Response channel
  output logic                                   noc_out_in_bid,
  output aerror_t                                noc_out_in_bresp,
  output logic        [`AXI_USER_RESP_WIDTH-1:0] noc_out_in_buser,
  output logic                                   noc_out_in_bvalid,
  // Read addr channel
  output logic                                   noc_out_in_arready,
  // Read data channel
  output logic                                   noc_out_in_rid,
  output logic        [`AXI_DATA_WIDTH-1:0]      noc_out_in_rdata,
  output aerror_t                                noc_out_in_rresp,
  output logic                                   noc_out_in_rlast,
  output logic        [`AXI_USER_REQ_WIDTH-1:0]  noc_out_in_ruser,
  output logic                                   noc_out_in_rvalid,

